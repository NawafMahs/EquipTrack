{
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest",
    "UseSsl": false,
    "ConnectionTimeoutSeconds": 30,
    "HeartbeatIntervalSeconds": 60,
    "MaxRetryAttempts": 5,
    "RetryDelayMilliseconds": 1000,
    "RobotCommandExchange": {
      "Name": "robot-commands",
      "Type": "topic",
      "Durable": true,
      "AutoDelete": false,
      "Arguments": {}
    },
    "RobotStatusExchange": {
      "Name": "robot-status",
      "Type": "topic",
      "Durable": true,
      "AutoDelete": false,
      "Arguments": {}
    },
    "CommandAckQueue": {
      "Name": "command-acknowledgments",
      "Durable": true,
      "Exclusive": false,
      "AutoDelete": false,
      "RoutingKey": "command.ack.#",
      "Arguments": {}
    },
    "StatusUpdateQueue": {
      "Name": "status-updates",
      "Durable": true,
      "Exclusive": false,
      "AutoDelete": false,
      "RoutingKey": "status.#",
      "Arguments": {}
    },
    "EnableMessagePersistence": true,
    "MessageTtlMilliseconds": 300000,
    "EnableDeadLetterExchange": true,
    "DeadLetterExchangeName": "dead-letter-exchange"
  }
}