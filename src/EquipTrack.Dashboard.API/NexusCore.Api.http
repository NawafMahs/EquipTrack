Authorization: Bearer {{token}}
Content-Type: application/json

{
  "title": "Updated Test Work Order",
  "description": "Updated test work order description",
  "priority": "High",
  "type": "Corrective",
  "scheduledDate": "2024-12-21T10:00:00Z",
